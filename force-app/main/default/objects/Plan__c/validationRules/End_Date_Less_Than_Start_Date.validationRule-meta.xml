<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Less_Than_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Plan_End_Date__c &lt; Plan_Start_Date__c</errorConditionFormula>
    <errorMessage>Plan End Date cannot be less than Plan Start Date.</errorMessage>
</ValidationRule>
